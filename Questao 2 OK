#include <stdio.h>
#include <stdlib.h>

int funcao(){

 int soma=0, i;
    while(i<=200){
        soma =soma + 7;
        i++;
        if(soma>200){
            printf("\n");
            return 0;
        }
        printf("%d, ", soma);   }
}

int main(){
   
funcao();


    return 0;
}
