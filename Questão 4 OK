#include <stdio.h>
#include <stdlib.h>

int funcao(){
  //
  int i=1, x;
  
    printf("Digite um numero para saber seus divisores: ");
    scanf("%d", &x);
    printf("os divisores de ");
    printf("%d ", x);
    printf("sao:");

    while(i<=x){
        if(x%i==0){
            printf("%d, ", i);
        }
        i++;
    }
}
//
int main(){
   
   funcao();
}
