#include <stdio.h>
#include <stdlib.h>

int funcao(){
  //
    int i=1, x, z;
    printf("Digita o numero que vc quer saber a tabuada: ");
    scanf("%d", &x);

    while(i<=10){
        z=x*i;
        printf("%d x %d = %d\n",x, i, z);
        i++;
    }
//
}
int main(){
   
   funcao();
}
