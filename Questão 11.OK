#include <stdio.h>
#include <stdlib.h>

int funcao(){
  //
      int i, x;
  int div = 0;

  do {
    system("clear");
    printf("Digite um numero inteiro e positivo: ");
    scanf("%d", &x);
  } while (x <= 0);

  for (i = 1; i <= x; i++) {
    if (x % i == 0) {
     div++;
    }
  }

  if (div == 2)
    printf("O numero %d eh primo!\n", x);
  else
    printf("O numero %d nao eh primo!\n", x);

//
}
int main(){
   
   funcao();
}
